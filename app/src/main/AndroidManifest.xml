<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ccs.myandriod">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.ccs.star.myandriod.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.ccs.star.myandriod.StarDetailActivity" android:label="星座详情"></activity>
        <activity android:name="com.ccs.star.myandriod.SelStarActivity" android:label="选择星座详情a"  android:theme="@android:style/Theme.Dialog"></activity>
        <activity android:name="com.ccs.star.myandriod.TestDialog"  android:label="Activity变sDialog"  android:theme="@android:style/Theme.Dialog"
            ></activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>